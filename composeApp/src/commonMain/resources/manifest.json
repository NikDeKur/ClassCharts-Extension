{
  "name": "ClassCharts",
  "description": "Extension for quick access to ClassCharts",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/"
  ]
}